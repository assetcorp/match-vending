{
	"name": "match-vending",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"dev": "next dev",
		"dev:test": "cross-env NODE_ENV=test node ./server/models/index.js && cross-env NODE_ENV=test next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"migrate": "./node_modules/.bin/sequelize db:create",
		"migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
		"app-migrate:reset": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:drop && cross-env NODE_ENV=test  ./node_modules/.bin/sequelize db:create",
		"test": "cross-env NODE_ENV=test jest --testTimeout=10000",
		"pretest": "cross-env NODE_ENV=test yarn app-migrate:reset",
		"posttest": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:drop"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		]
	},
	"repository": "https://github.com/assetcorp/match-vending.git",
	"author": "Delali <michaelazumah@live.com>",
	"license": "MIT",
	"private": false,
	"dependencies": {
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"dinero.js": "^1.9.1",
		"dotenv": "^10.0.0",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"mysql2": "^2.3.3",
		"next": "^12.0.7",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"sequelize": "^6.12.0-beta.3",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"babel-register": "^6.26.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.4.1",
		"eslint-plugin-react": "^7.27.1",
		"jest": "^27.4.5",
		"pm2": "^5.1.2",
		"sequelize-cli": "^6.3.0",
		"supertest": "^6.1.6"
	}
}
